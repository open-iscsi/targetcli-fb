rm -v rtsadmin/*.pyc rtsadmin/*.html 2>/dev/null
rm -rvf doc 2>/dev/null
rm -rfv rtsadmin.egg-info MANIFEST build dist 2>/dev/null
rm -rvf pdf html 2>/dev/null
rm -rvf debian/tmp 2>/dev/null
rm -v build-stamp 2>/dev/null
rm -v dpkg-buildpackage.log dpkg-buildpackage.version 2>/dev/null
rm -rfv *.rpm warnrtsadmin.txt buildrtsadmin 2>/dev/null
rm -v debian/*.debhelper.log debian/*.debhelper debian/*.substvars debian/files 2>/dev/null
rm -vrf debian/rtsadmin-frozen/ debian/rtsadmin-python2.5/ debian/rtsadmin-python2.6/	debian/rtsadmin/ debian/rtsadmin-doc/ 2>/dev/null

echo "Finished cleanup."

